user_find('email_address','APPID')
user_create('email_address',"AP'PID")
user_set('u_name','Peder')
user_create('bingo@barcelona.com',"SPONG")
user_first()
user_get('u_name')
user_next()
user_get('u_name')
user_next()
user_find('email_address','AP\'PID')
dset_create('DSET','KEY')
user_create('viggo@barcelona.com',"SPONG")
user_set('u_name','Viggo Hansen')
dset_find('AP\'PID','DSET')
dset_first()
dset_next()
infoDS_put('LOCATION','/my/directory')
infoDS_get('LOCATION')
file_find('myfile')
file_create('myfile')
file_set('f_timestamp','2011-01-01')
file_first()
file_get('f_timestamp')
file_next()
file_find('myfile')
dset_create('DS-NEW','KEY')
infoDS_put('LOCATION','/my/somewhere')
infoDS_put('CATALOG','http://thredds/somewhere')
dset_first()
infoDS_get('LOCATION')
infoDS_put('CATALOG','http://thredds/somewhereelse')
user_find('email_address','AP\'PID')
infoUDS_set("SUBSCRIPTION_XML","USER_AND_DATASET")
infoUDS_new("<XML>with an embedded apostrophe: '</XML>")
infoUDS_new("<subs>Another XML</subs>")
user_find('bingo@barcelona.com','SPONG')
infoUDS_set("SUBSCRIPTION_XML","USER")
infoUDS_new("<XML>for a new user</XML>")
infoUDS_new('<subs>Another XML for user bingo@barcelona.com</subs>')
dset_next()
infoUDS_new("<subs>This is for dataset 'DS-NEW'</subs>")
dset_find('AP\'PID','DSET')
infoUDS_new("<subs>This is for dataset 'DSET'</subs>")
infoUDS_set("SUBSCRIPTION_XML","DATASET")
user_isync()
infoUDS_get()
user_get('u_name')
infoUDS_next()
user_isync()
infoUDS_get()
user_get('u_name')
infoUDS_set("SUBSCRIPTION_XML","USER")
dset_isync()
infoUDS_get()
infoUDS_delete()
infoUDS_next()
infoUDS_set("SUBSCRIPTION_XML","USER")
infoUDS_get()
dset_isync()
infoUDS_get()
user_dsync()
close()
exit
