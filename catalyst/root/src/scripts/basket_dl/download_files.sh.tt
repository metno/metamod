#!/bin/sh
# script generated by METAMOD collection basket

if hash curl 2>/dev/null; then
    CMD="curl -O"
elif hash wget 2>/dev/null; then
    CMD="wget"
else
    echo No download program found in PATH >&2
    exit 1
fi

[%# both curl and wget support multiple urls on invocation which should be faster... FIXME %]
[% FOREACH url IN links -%]
$CMD '[% url %]'
[% END %]
